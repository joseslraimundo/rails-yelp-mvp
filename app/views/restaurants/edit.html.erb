<%= form_for @restaurant do |r| %>
  <%= r.label :name, "Enter name" %>
  <%= r.text_field :name %>
  <%= r.label :address, "Enter address" %>
  <%= r.text_field :address %>
  <%= r.label :phone_number, "Enter phone number" %>
  <%= r.text_field :phone_number %>
  <%= r.label :category, "Enter category" %>
  <%= r.text_field :category %>
  <%= r.submit%>
<% end %>
